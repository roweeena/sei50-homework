<h3>Current Rental Listings Around North Strathfield</h3>

<div class="list-wrapper">
    <div class="col-heading">Address</div>
    <div class="col-heading">Net Cost ($<%  %>/week)</div>
    <div class="col-heading">Cost ($/week)</div>
    <div class="col-heading">Travel Time (hours)</div>
    <div class="col-heading">Travel Time Cost ($)</div>
    <% @rentals.each do |rental| %>
    <div class="grid-item"><a href="/rental/<%= rental.id %>"><%= rental.street_address + ", " + rental.suburb %></a></div>
    <div class="grid-item">$<%= rental.net_cost %></div>
    <div class="grid-item">$<%= rental.cost %></div>
    <div class="grid-item"><%= rental.total_travel_time %></div>
    <div class="grid-item"><%= rental.total_time_value %></div>
    <% end %>
</div>

</ul>